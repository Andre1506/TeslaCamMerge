[Unit]
Description=Start filebrowser
After=network.target
After=systemd-user-session.service
After=mergeTeslaCam.service
RequiresMountsFor=SSD_MOUNT_POINT

[Service]
User=PROJECT_USER
ExecStart=FILEBROWSER_PATH -d PROJECT_PATH/filebrowser.db
Restart=on-failure
RestartSec=60

[Install]
WantedBy=multi-user.target
